    "value=[1,3,6,9]\n",
    "print(\"list:\")\n",
    "print(value)\n",
    "def shift(list):\n",
    "    list1=[]\n",
    "    for i in list:\n",
    "        list1.insert(len(list1)-1,i)\n",
    "    return list1\n",
    "print(\"list after shifted left by one position:\")\n",
    "print(shift(value))"
